{
  "manifest_version": 2,

  "name": "New Tab Game",
  "description": "Some stress relief everytime you open a new tab.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "New Tab"
  },
  "chrome_url_overrides": {
		"newtab": "index.html"
	},
  "permissions": [
    "storage"
  ]
}